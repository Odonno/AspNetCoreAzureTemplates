<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
  <metadata>
    <id>AspNetCoreAzureTemplates</id>
    <version>1.0.0</version>
    <description>
      Creates an ASP.NET Core Web App using ready-to-use Azure features (Healthchecks, SignalR, Azure SignalR, Azure AD, Microsoft Graph API).
    </description>
    <authors>David Bottiau</authors>
    <packageTypes>
      <packageType name="Template" />
    </packageTypes>
  </metadata>
  <files>
    <file src=".\**" target="content" exclude="**\bin\**;**\obj\**;**\.vs\**;**\*.user;**\.git\**"/>
    <file src=".\.template.config\template.json" target="content\.template.config\template.json"/>
  </files>
</package>